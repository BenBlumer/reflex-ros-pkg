<!-- nodes and parameters for reflex -->
<launch>
  <rosparam file="$(find reflex_actuators)/reflex.yaml" command="load"/>
  <node pkg="takktile_ros" type="takktile_node.py" name="takktile" output="screen" respawn="true"/>

  <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                reflex_port:
                    port_name: "/dev/ttyUSB0"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 5
                    update_rate: 20
        </rosparam>
  </node>
  <node name="reflex_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port reflex_port
                reflex_f1
                                reflex_f2
                                reflex_f3
                                reflex_rotate"
          output="screen"/>
</launch>

